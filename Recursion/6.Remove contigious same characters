#include<iostream>
using namespace std;

void remove(char input[]){
    int size=strlen(input);
    if(size==0||size==1)
    return;
    if(input[0]==input[1]){
        for(int i=1;i<=size;i++)
        input[i-1]=input[i];
        remove(input);
    }
    else
        remove(input+1);
}

int main(){
    char input[100];
    cout<<"enter the string:";
    cin>>input;
    remove(input);
    cout<<input;
}
