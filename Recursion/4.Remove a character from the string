#include<iostream>
#include<string.h>
using namespace std;
void replace(char str[],int size){
    if(str[0]=='\0')
    return ;
    if(str[0]=='x'){
        for(int i=0;i<size;i++)
        str[i]=str[i+1];
        replace(str,size);
    }   
    else
    return replace(str+1,size-1);
}
int main(){
    char str[100];
    cout<<"enter the string...";
    cin>>str;
    int l=strlen(str);
    replace(str,l);
    cout<<str;

}
